  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
    "check": "flow check",
    "test": "jest --env=node",
    "build": "node scripts/build.js",
    "build-watch": "node scripts/build.js --watch",
    "db": "./scripts/db.sh",
    "db-version": "knex migrate:currentVersion",
    "db-migrate": "knex migrate:latest",
    "db-rollback": "knex migrate:rollback",
    "db-change": "node scripts/db.js migration",
    "db-migration": "node scripts/db.js migration",
    "db-seed": "node scripts/db.js seed",
    "db-restore": "./scripts/db-restore.sh",
    "update-schema": "node scripts/update-schema.js",
    "heroku-db-migrate": "yarn install --production=false --force && knex migrate:latest",
    "heroku-db-rollback": "yarn install --production=false --force && knex migrate:rollback",
    "heroku-postbuild": "yarn install --production=false && yarn build",
    "start": "node --icu-data-dir=`yarn -s run node-full-icu-path` ./server.js"
  }
